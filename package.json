{
  "name": "autocanvas",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "yarpm run test && yarpm run compile:webpack",
    "compile:webpack": "webpack",
    "lint:sort": "sort-package-json",
    "lint:eslint": "eslint . --fix --ext .js,.ts,.mjs,.mts,.cjs,.cts,.tsx,.jsx",
    "lint": "yarpm run lint:sort && yarpm run lint:eslint",
    "package": "yarpm run test && yarpm run package:webpack",
    "package:webpack": "webpack --mode production --devtool hidden-source-map",
    "release": "release-it",
    "pretest": "yarpm run clean && yarpm run typecheck-root && yarpm run lint",
    "test": "jest",
    "typecheck": "yarpm run typecheck-root && tsc --noEmit -p tsconfig.json",
    "typecheck-root": "tsc -p ./tsconfig.root.json",
    "prewatch": "yarpm run clean",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "bookmarklet": "^3.0.0",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.2",
    "sort-package-json": "^2.2.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yarpm": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributers": [
    "Aaron Dill"
  ]
}
